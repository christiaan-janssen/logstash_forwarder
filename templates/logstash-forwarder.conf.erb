{
	"network": {
		"servers": [ "192.168.0.11:5000" ],
		"timeout": 15,
		"ssl ca": "/etc/pki/tls/certs/logstash-forwarder.crt"
	},
	"files": [
    {
  		"paths": [
  			"/var/log/syslog",
  			"/var/log/auth.log"
  		],
  		"fields": { "type": "syslog" }
  	},
    {
  		"paths": [ "/var/www/vhosts/fng/httpdocs/var/log/exception.log" ],
  		"fields": { "type": "magento-exception"}
  	},
    {
  		"paths": [ "/var/www/vhosts/fng/httpdocs/var/log/system.log" ],
  		"fields": { "type": "magento-system"}
	  }
  ]
}
